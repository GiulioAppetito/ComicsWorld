<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="150.0" prefWidth="100.0" stylesheets="@../../../../css/fullpackstyling.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.comics.view1.VCardController">
    <styleClass>
        <String fx:value="white-background" />
        <String fx:value="transparent" />
    </styleClass>
    <ImageView fx:id="comicCover" fitHeight="183.0" fitWidth="122.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../../../../img/2.jpg" />
      <VBox.margin>
         <Insets />
      </VBox.margin>
    </ImageView>
    <Label fx:id="comicName" alignment="CENTER" prefHeight="38.0" prefWidth="107.0" text="Spiderman">
        <font>
            <Font name="Berlin Sans FB" size="14.0" />
        </font>
    </Label>
</VBox>
