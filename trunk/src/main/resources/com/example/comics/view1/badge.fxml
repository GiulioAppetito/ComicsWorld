<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="101.0" prefWidth="82.0" stylesheets="@../../../../css/fullpackstyling.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.comics.view1.BadgeCardControllerG">
   <children>
      <ImageView fx:id="badgeIcon" fitHeight="70.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../img/download.png" />
         </image>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </ImageView>
      <Label fx:id="badgeName" alignment="CENTER" prefHeight="22.0" prefWidth="106.0" text="Badge">
         <font>
            <Font name="Berlin Sans FB" size="10.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </Label>
   </children>
   <styleClass>
      <String fx:value="oval-layout" />
      <String fx:value="onlyhover" />
   </styleClass>
</VBox>
