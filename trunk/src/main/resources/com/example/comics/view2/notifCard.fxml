<VBox alignment="CENTER" prefHeight="99.0" prefWidth="248.0" stylesheets="@../../../../css/css1/fullpackstyling.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.comics.view2.NotifcationCardControllerG">
   <styleClass>
      <String fx:value="white-background" />
      <String fx:value="oval-layout" />
      <String fx:value="button2" />
   </styleClass>
   <children>
      <HBox prefHeight="85.0" prefWidth="231.0">
         <VBox.margin>
            <Insets bottom="8.0" top="8.0" />
         </VBox.margin>
