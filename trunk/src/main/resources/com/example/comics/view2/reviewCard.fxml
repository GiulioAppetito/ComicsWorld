<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="119.0" prefWidth="214.0" stylesheets="@../../../../css/css1/fullpackstyling.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.comics.view2.ReviewCardControllerG">
   <styleClass>
      <String fx:value="white-background" />
      <String fx:value="oval-layout" />
      <String fx:value="button2" />
   </styleClass>
   <children>
      <HBox alignment="CENTER">
         <children>
            <ImageView fx:id="propic" fitHeight="102.0" fitWidth="102.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../img/img1/flat-business-man-user-profile-avatar-icon-vector-4333097.jpg" />
               </image>
            </ImageView>
            <VBox prefHeight="104.0" prefWidth="110.0">
               <children>
                  <Label fx:id="username" alignment="CENTER" prefHeight="35.0" prefWidth="93.0" text="AuthorName">
                     <font>
                        <Font name="Berlin Sans FB" size="14.0" />
                     </font>
                  </Label>
                  <HBox>
                     <children>
                        <Label fx:id="comment1" prefHeight="17.0" prefWidth="46.0" text="Rating : " />
                        <Label fx:id="lblRating" layoutX="10.0" layoutY="10.0" prefHeight="17.0" prefWidth="46.0" text="vote" />
                     </children>
                  </HBox>
                  <ProgressBar fx:id="ratingBar" prefHeight="19.0" prefWidth="100.0" progress="0.0">
                     <VBox.margin>
                        <Insets right="10.0" />
                     </VBox.margin>
                  </ProgressBar>
                  <Label fx:id="comment" prefHeight="46.0" prefWidth="100.0" text="Commento" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
