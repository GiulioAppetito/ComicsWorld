<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="119.0" prefWidth="259.0" stylesheets="@../../../../css/css1/fullpackstyling.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.comics.view2.OrderCardControllerG2">
   <styleClass>
      <String fx:value="white-background" />
      <String fx:value="oval-layout" />
      <String fx:value="button2" />
   </styleClass>
   <children>
      <HBox alignment="CENTER">
         <children>
            <ImageView fx:id="seriesCover" fitHeight="102.0" fitWidth="102.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../img/img1/2-1.jpg" />
               </image>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ImageView>
            <VBox prefHeight="104.0" prefWidth="83.0">
               <children>
                  <Label fx:id="seriesTitle" alignment="CENTER" prefHeight="30.0" prefWidth="112.0" text="Series">
                     <font>
                        <Font name="Berlin Sans FB" size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="lblExpense" alignment="CENTER" prefHeight="18.0" prefWidth="110.0" text="expense">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="lblDate" alignment="CENTER" prefHeight="18.0" prefWidth="110.0" text="Date">
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Label>
               </children>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </VBox>
            <VBox>
               <children>
                  <Label fx:id="chapterTitle" alignment="CENTER" prefHeight="30.0" prefWidth="74.0" text="chapter">
                     <font>
                        <Font name="Berlin Sans FB" size="14.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
