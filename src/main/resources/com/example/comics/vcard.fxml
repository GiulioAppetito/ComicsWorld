<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="100.0" prefWidth="80.0" stylesheets="@../../../css/fullpackstyling.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.comics.VCardController">
    <styleClass>
        <String fx:value="white-background" />
        <String fx:value="oval-layout" />
      <String fx:value="transparent" />
    </styleClass>
    <ImageView fx:id="comicCover" fitHeight="54.0" fitWidth="71.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../../../img/spidey.png" />
      <VBox.margin>
         <Insets top="5.0" />
      </VBox.margin>
    </ImageView>
    <Label fx:id="comicName" alignment="CENTER" prefHeight="38.0" prefWidth="107.0" text="Spiderman">
        <font>
            <Font name="Berlin Sans FB" size="14.0" />
        </font>
    </Label>
</VBox>
