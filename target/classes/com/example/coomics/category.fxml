<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="90.0" prefWidth="800.0" spacing="15.0" stylesheets="@../../../css/fullpackstyling.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.coomics.CategoryController">
    <styleClass>
        <String fx:value="oval-layout" />
        <String fx:value="transparent" />
    </styleClass>
    <ImageView fitHeight="72.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true">
        <HBox.margin>
            <Insets bottom="10.0" left="10.0" top="15.0"/>
        </HBox.margin>

        <Image url="@../../../img/output-onlinepngtools.png"/>
    </ImageView>
    <Label fx:id="categoryName" alignment="CENTER" prefHeight="32.0" prefWidth="179.0" text="Action">
        <HBox.margin>
            <Insets bottom="30.0" left="15.0" top="35.0" />
        </HBox.margin>
        <font>
            <Font size="18.0" />
        </font>
    </Label>
</HBox>
